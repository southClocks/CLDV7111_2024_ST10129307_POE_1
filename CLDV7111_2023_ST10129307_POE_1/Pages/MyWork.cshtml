@page
@model CLDV7111_2023_ST10129307_POE_1.Pages.MyWorkModel
@{
    ViewData["Title"] = "My Work";
}
<div class="container mt-5">
<h2>@ViewData["Title"]</h2>
</div>


<div class="container mt-5">
    <h2>Our Handcrafted Products</h2>
    <div class="row">
        <div class="col-md-4 mt-3">
            <div class="card">
                <img src="~/images/Dylan.png" class="card-img-top" alt="Product 1">
                <div class="card-body">
                    <h5 class="card-title">Handbag</h5>
                    <p>hand bag with beautifull colors.</p>
                    <p class="product-price">Price: R780</p>
                    <a href="#" class="btn btn-custom">View More</a>
                </div>
            </div>
        </div>

        <div class="col-md-4 mt-3">
            <div class="card">
                <img src="~/images/Cotton_candy.png" class="card-img-top" alt="Product 2">
                <div class="card-body">
                    <h5 class="card-title">Cotton Candy bracelet</h5>
                    <p>a bracelet, all sizes</p>
                    <p class="product-price">Price: R300</p>
                    <a href="#" class="btn btn-custom">View More</a>
                </div>
            </div>
        </div>

        <div class="col-md-4 mt-3">
            <div class="card">
                <img src="~/images/warsha1_a_carved_oil_lamp_.png" class="card-img-top" alt="Oil lamp">
                <div class="card-body">
                    <h5 class="card-title">Oil lamp</h5>
                    <p>designer carved oil lamp</p>
                    <p class="product-price">Price: R937</p>
                    <a href="#" class="btn btn-custom">View More</a>
                </div>
            </div>
        </div>

    </div>
</div>



